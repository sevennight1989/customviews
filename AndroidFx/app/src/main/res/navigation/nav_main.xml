<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/fg_main">
    <fragment
        android:id="@+id/fg_main"
        android:name="com.android.custview.jetpack.fragment.MainFragment"
        android:label="MainFragment"
        tools:layout="@layout/main_fragment">
    </fragment>
    <action
        android:id="@+id/nav_to_login"
        app:destination="@id/fg_login"
        app:launchSingleTop="true" />
    <fragment
        android:id="@+id/fg_login"
        android:name="com.android.custview.jetpack.fragment.LoginFragment"
        android:label="LoginFragment"
        tools:layout="@layout/jetpack_login_main" />
</navigation>