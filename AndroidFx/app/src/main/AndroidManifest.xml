<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.custview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.com.android.custview.ui.JetPackActivity"></activity>
        <activity android:name="com.com.android.custview.ui.ScrollActivity" />
        <activity android:name="com.com.android.custview.ui.CardViewActivity" />
        <activity android:name="com.com.android.custview.ui.MarqueeActivity" />
        <activity android:name="com.com.android.custview.ui.ListActivity" />
        <activity android:name="com.com.android.custview.ui.DraggerActivity" />
        <activity android:name="com.com.android.custview.ui.CustViewGroupActivity" />
        <activity android:name="com.com.android.custview.ui.CustomVolumActivity" />
        <activity android:name="com.com.android.custview.ui.ProgressBarActivity" />
        <activity android:name="com.com.android.custview.ui.CustView01Activity" />
        <activity android:name="com.com.android.custview.ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>